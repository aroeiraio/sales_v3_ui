{
	"name": "sales-v3-ui",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node build/server/index.js",
		"docker:build": "docker build -t sales-v3-ui .",
		"docker:run": "docker run -p 8090:8090 sales-v3-ui",
		"docker:compose:up": "docker-compose up -d",
		"docker:compose:down": "docker-compose down",
		"docker:compose:logs": "docker-compose logs -f",
		"docker:compose:build": "docker-compose up -d --build",
		"docker:compose:restart": "docker-compose restart",
		"docker:compose:ps": "docker-compose ps",
		"docker:compose:exec:frontend": "docker-compose exec sales_frontend sh",
		"docker:compose:exec:api": "docker-compose exec sales_api sh",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"eslint": "^9.35.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vite-plugin-pwa": "^1.0.3",
		"vitest": "^3.2.4",
		"workbox-window": "^7.3.0"
	},
	"dependencies": {
		"lucide-svelte": "^0.542.0"
	}
}
